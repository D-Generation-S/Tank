<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="DebugGui.Views.ComponentsView">
	<StackPanel>
		<ListBox Items="{Binding Components}" SelectedItem="{Binding SelectedComponentView}"/>
		<StackPanel Orientation="Horizontal">
			<Button Content="Collapse All" Command="{Binding CollapseAllCommand}"/>
			<Button Content="Expand All" Command="{Binding ExpandAllCommand}"/>
		</StackPanel>
	</StackPanel>
</UserControl>
